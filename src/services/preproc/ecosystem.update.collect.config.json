{
    "apps": [{
        "name": "X5GON Material Update Collector",
        "script": "pipeline.js",
        "cwd": "./pipelines/",
        "args": "-tn uuid.update.material.select -tp ./ontologies/update.material.select",
        "env": {
            "NODE_ENV": "development"
        },
        "env_production": {
            "NODE_ENV": "production"
        },
        "instances": "1",
        "exec_mode": "cluster",
        "autorestart": true,
        "max_restarts": 20
    }]
}
