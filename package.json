{
  "name": "x5gon",
  "version": "1.0.0",
  "description": "Connecting OER repositories",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run minify:snippet && git checkout -- src/lib/textract && npm run install:textract && npm audit fix",
    "start": "npm run pm2:platform && npm run pm2:recsys && npm run pm2:preproc",
    "start-dev": "npm run pm2:platform-dev && npm run pm2:recsys-dev && npm run pm2:preproc-dev",
    "postgres:create": "cd src/load && node -e 'require(`./create-postgres-database`).startDBCreate();'",
    "minify:snippet": "cd src/server/platform/scripts && node minify-snippet.js",
    "install:textract": "cd src/lib/textract && npm install && npm audit fix",
    "react:start": "cd src/server/platform/frontend && npm start",
    "react:build": "cd src/server/platform/frontend && npm run-script build",
    "test": "cd src/test && NODE_ENV=test mocha all.js",
    "test:lib": "cd src/test/lib && NODE_ENV=test mocha *.js",
    "test:recsys": "cd src/server/recsys/test && NODE_ENV=test mocha *.js",
    "pm2:platform": "pm2 reload ecosystem.platform.config.json --env production",
    "pm2:recsys": "pm2 reload ecosystem.recsys.config.json --env production",
    "pm2:preproc": "cd src/server/preproc && pm2 reload ecosystem.collect.config.json --env production && pm2 reload ecosystem.process.text.config.json --env production && pm2 reload ecosystem.process.video.config.json --env production && pm2 reload ecosystem.store.config.json --env production",
    "pm2:platform-dev": "pm2 reload ecosystem.platform.config.json",
    "pm2:recsys-dev": "pm2 reload ecosystem.recsys.config.json",
    "pm2:preproc-dev": "cd src/server/preproc && pm2 reload ecosystem.collect.config.json && pm2 reload ecosystem.process.text.config.json && pm2 reload ecosystem.process.video.config.json && pm2 reload ecosystem.store.config.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JozefStefanInstitute/x5gon.git"
  },
  "author": "Erik Novak, Jasna Urbančič",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/JozefStefanInstitute/x5gon/issues"
  },
  "homepage": "https://github.com/JozefStefanInstitute/x5gon#readme",
  "dependencies": {
    "@elastic/elasticsearch": "^7.4.0",
    "archiver": "^3.0.0",
    "async": "^2.6.0",
    "body-parser": "^1.18.2",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.16.2",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.15.6",
    "file-type": "^8.0.0",
    "gatsby-plugin-express": "^1.1.6",
    "handlebars": "^4.5.1",
    "handlebars-helper-sri": "0.0.0",
    "http-proxy-middleware": "^0.18.0",
    "jsonschema": "^1.2.2",
    "kafka-node": "^4.1.3",
    "mime-types": "^2.1.21",
    "module-alias": "^2.2.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.4.1",
    "pg-cursor": "^1.3.0",
    "pm2": "^3.0.3",
    "python-shell": "^0.5.0",
    "qminer": "^9.2.4",
    "qtopology": "^2.2.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "socket.io": "^2.1.1",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.8.0",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "mocha": "^5.2.0",
    "node-minify": "^2.4.1",
    "supertest": "^3.3.0",
    "ua-parser-js": "^0.7.18"
  },
  "_moduleAliases": {
    "@library": "src/lib",
    "@config": "src/config",
    "@platform_schemas": "src/server/platform/schemas",
    "@recsys_schemas": "src/server/recsys/schemas"
  }
}
